import matplotlib.pyplot as plt
import math
import numpy as np


def norma(A,B):
    if len(A) != len(B):
        print("vetores de tamanhos diferentes")
        return
    soma = 0.0
    for i in range(len(A)):
        soma = soma + (A[i]-B[i])**2
    soma = np.sqrt(soma)
    return soma

def main():
    
    for i in range(4):
        n = 16*2**(i)
        string = "n = " + str(n)
        print(string)


main()